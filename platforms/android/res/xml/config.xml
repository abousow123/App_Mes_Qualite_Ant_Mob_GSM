    <splash density="land-ldpi" src="www/img/splash-xxxhdpi.png" />
    <splash density="land-mdpi" src="www/img/splash-xxxhdpi.png" />
    <splash density="land-xhdpi" src="www/img/splash-xxxhdpi.png" />
    <splash density="port-hdpi" src="www/img/splash-xxxhdpi.png" />
    <splash density="port-ldpi" src="www/img/splash-xxxhdpi.png" />
    <splash density="port-mdpi" src="www/img/splash-xxxhdpi.png" />
